<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".my_profile"
    android:theme="@android:style/Theme.Translucent"
tools:background="@drawable/signup">


<RelativeLayout
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="80sp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.004"
    tools:layout_editor_absoluteX="0dp">


</RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout">

        <LinearLayout
            android:id="@+id/content01"
            android:layout_width="match_parent"
            android:layout_height="550sp"
            android:background="@drawable/backgrounsignup"
            android:gravity="center"
            android:orientation="vertical"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintVertical_bias="0.0"
            tools:layout_editor_absoluteX="0dp">


            <TextView
                android:id="@+id/profile"
                android:layout_width="337sp"
                android:layout_height="53sp"
                android:layout_gravity="center"
                android:layout_marginBottom="10sp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Update Profile"
                android:textColor="@color/black"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.759" />


            <TextView
                android:id="@+id/orders"
                android:layout_width="337sp"
                android:layout_height="53sp"
                android:layout_gravity="center"
                android:layout_marginBottom="10sp"
                android:clickable="true"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Your Orders"
                android:textColor="@color/black"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.464" />

            <TextView
                android:id="@+id/Privacy"
                android:layout_width="337sp"
                android:layout_height="53sp"
                android:layout_gravity="center"
                android:layout_marginBottom="10sp"
                android:clickable="true"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Privacy"
                android:textColor="@color/black"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.661" />

            <TextView
                android:id="@+id/about_us"
                android:layout_width="337sp"
                android:layout_height="53sp"
                android:layout_gravity="center"
                android:layout_marginBottom="10sp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="About Us"
                android:textColor="@color/black"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.759" />

            <TextView
                android:id="@+id/password_change"
                android:layout_width="337sp"
                android:layout_height="53sp"
                android:layout_gravity="center"
                android:layout_marginBottom="10sp"
                android:clickable="true"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Change Password"
                android:textColor="@color/black"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.568" />

            <TextView
                android:id="@+id/feedback"
                android:layout_width="337sp"
                android:layout_height="53sp"
                android:layout_gravity="center"
                android:layout_marginBottom="10sp"
                android:clickable="true"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Give Feedback"
                android:textColor="@color/black"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.862" />

            <TextView
                android:id="@+id/Cart"
                android:layout_width="337sp"
                android:layout_height="53sp"
                android:layout_gravity="center"
                android:clickable="true"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Your Cart"
                android:textColor="@color/black"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.349" />

            <Button
                android:layout_marginTop="15dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/brown"
                android:textColor="@color/white"
                android:text="LOGOUT"
                android:textSize="15dp"
                android:id="@+id/logout"
                />


        </LinearLayout>
    </RelativeLayout>



</androidx.constraintlayout.widget.ConstraintLayout>
